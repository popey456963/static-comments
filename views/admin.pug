extends layout

block content
  .container
    .panel
      .panel-heading
        h3.panel-title List of Pages
      .panel-body
        if messages.success
          .alert.alert-success(role='alert')
            for success in messages.success
              div= success.msg
        if messages.error
          .alert.alert-danger(role='alert')
            for error in messages.error
              div= error.msg

        for page in pages
          p= page
